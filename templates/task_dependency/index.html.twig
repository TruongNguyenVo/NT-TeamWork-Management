{% extends 'base.html.twig' %}

{% block title %}TaskDependency index{% endblock %}

{% block body %}
    <h1>TaskDependency index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for task_dependency in task_dependencies %}
            <tr>
                <td>{{ task_dependency.id }}</td>
                <td>
                    <a href="{{ path('app_task_dependency_show', {'id': task_dependency.id}) }}">show</a>
                    <a href="{{ path('app_task_dependency_edit', {'id': task_dependency.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_task_dependency_new') }}">Create new</a>
{% endblock %}
